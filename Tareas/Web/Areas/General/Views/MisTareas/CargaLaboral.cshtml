@using (Html.BeginForm())
{
    <div class="box ">
        <div class="box-header with-border">
            <div class="row">
                <div class="col-md-12">
                    <button type="submit" class="btn btn-primary">Buscar</button>
                </div>
            </div>
            <div class="box-tools pull-right">
                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
                    <i class="fa fa-minus"></i>
                </button>
                <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                    <i class="fa fa-times"></i>
                </button>
            </div>
        </div>

        <div class="box-body">
            <div class="row">
                <div class="col-md-12">


                    <div class="col-md-4">
                        <div class="form-group">
                            <label for="IdUsuario">Usuario*</label>
                            @Html.DevExpress().ComboBoxFor(q => q.IdUsuario, settings =>
                       {
                           settings.Name = "IdUsuario";
                           settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);
                           settings.Properties.DisplayFormatString = "[{0}],[{1}]";
                           settings.Properties.Columns.Add("IdUsuario", "Usuario").Width = System.Web.UI.WebControls.Unit.Percentage(25);
                           settings.Properties.Columns.Add("Nombre", "Nombre").Width = System.Web.UI.WebControls.Unit.Percentage(75);
                           //Campo ID
                           settings.Properties.ValueField = "IdUsuario";
                           settings.Properties.ValueType = typeof(string);
                           settings.Properties.TextField = "Nombre";
                           //Boton para seleccionar ningun registro
                           settings.Properties.ClearButton.DisplayMode = ClearButtonDisplayMode.Never;
                           settings.Properties.NullText = "== Seleccione ==";
                           settings.Properties.DropDownStyle = DropDownStyle.DropDown;
                           settings.Properties.ClientSideEvents.SelectedIndexChanged = "function(s,e){vaciar_grupo();}";

                       }).BindList(ViewBag.list_usuario).GetHtml()
                            @Html.ValidationMessageFor(q => q.IdUsuario, "", new { @class = "text-danger" })
                        </div>
                    </div>

                    <div class="col-md-4">
                        <div class="form-group">
                            <label for="IdGrupoFiltro">Grupo*</label>
                            @Html.DevExpress().ComboBoxFor(q => q.IdGrupoFiltro, settings =>
                       {
                           settings.Name = "IdGrupoFiltro";
                           settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);
                           settings.Properties.DisplayFormatString = "[{0}],[{1}]";
                           settings.Properties.Columns.Add("IdGrupo", "ID").Width = System.Web.UI.WebControls.Unit.Percentage(10);
                           settings.Properties.Columns.Add("Descripcion", "Grupo").Width = System.Web.UI.WebControls.Unit.Percentage(90);
                           //Campo ID
                           settings.Properties.ValueField = "IdGrupoFiltro";
                           settings.Properties.ValueType = typeof(int);
                           settings.Properties.TextField = "Descripcion";
                           //Boton para seleccionar ningun registro
                           settings.Properties.ClearButton.DisplayMode = ClearButtonDisplayMode.Always;
                           settings.Properties.NullText = "== Seleccione ==";
                           settings.Properties.DropDownStyle = DropDownStyle.DropDownList;
                           settings.Properties.ClientSideEvents.SelectedIndexChanged = "function(s,e){vaciar_usuario();}";

                       }).BindList(ViewBag.list_grupo).GetHtml()
                            @Html.ValidationMessageFor(q => q.IdGrupoFiltro, "", new { @class = "text-danger" })
                        </div>
                    </div>

                    <div class="col-md-2">
                        <div class="form-group">
                            <label for="FechaEntrega">Fecha corte*</label>
                            <div class="input-group date">
                                <div class="input-group-addon">
                                    <i class="fa fa-calendar"></i>
                                </div>
                                @Html.DevExpress().DateEditFor(q => q.FechaInicio, settings =>
                           {
                               settings.Name = "FechaInicio";
                               settings.Properties.UseMaskBehavior = true;
                               settings.Properties.EditFormat = EditFormat.Date;
                               settings.Properties.EditFormatString = "dd/MM/yyyy";
                               settings.Properties.TimeSectionProperties.TimeEditProperties.EditFormat = EditFormat.Date;
                               settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);
                           }).GetHtml()
                                @Html.ValidationMessageFor(q => q.FechaInicio, "", new { @class = "text-danger" })
                            </div>
                        </div>
                    </div>


                </div>
            </div>
        </div>
        <!-- /.box-body -->
    </div>

    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">@ViewBag.Title</h3>
            <div class="box-tools pull-right">
                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
                    <i class="fa fa-minus"></i>
                </button>
                <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                    <i class="fa fa-times"></i>
                </button>
            </div>
        </div>
        <div class="box-body">

            <div class="row">
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-red">
                        <div class="inner">
                            <h3>@Model.NumTareaVencidas</h3>

                            <p>Vencidas</p>
                        </div>
                        <div class="icon">
                            <i class="ion-ios-alarm"></i>
                        </div>
                        @if (Model.IdUsuario == Web.Helps.SessionTareas.IdUsuario)
                        {
                            <a href="@Url.Action("Buzon_entrada","Buzon")" class="small-box-footer"> Resolver tareas <i class="fa fa-arrow-circle-right"></i></a>
                        }
                        else
                        {
                            <a href="#" class="small-box-footer"> Resolver tareas <i class="fa fa-arrow-circle-right"></i></a>
                        }
                    </div>
                </div>

                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-yellow">
                        <div class="inner">
                            <h3>@Model.NumTareaPorAprobar</h3>

                            <p>Pendiente de aprobar</p>
                        </div>
                        <div class="icon">
                            <i class="ion-ios-clock"></i>
                        </div>
                        @if (Model.IdUsuario == Web.Helps.SessionTareas.IdUsuario)
                        {
                            <a href="@Url.Action("Por_aprobar")" class="small-box-footer"> Aprobar tareas <i class="fa fa-arrow-circle-right"></i></a>
                        }
                        else
                        {
                            <a href="#" class="small-box-footer"> Aprobar tareas <i class="fa fa-arrow-circle-right"></i></a>
                        }
                    </div>
                </div>

                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-green">
                        <div class="inner">
                            <h3>@Model.TotalTareaResueltas<sup style="font-size: 20px"></sup></h3>

                            <p>Resueltas</p>
                        </div>
                        <div class="icon">
                            <i class="icon ion-android-checkbox-outline"></i>
                        </div>
                        @if (Model.IdUsuario == Web.Helps.SessionTareas.IdUsuario)
                        {
                            <a href="@Url.Action("Buzon_entrada","Buzon", new { Estado="A"})" class="small-box-footer"> Resolver tareas <i class="fa fa-arrow-circle-right"></i></a>
                        }
                        else
                        {
                            <a href="#" class="small-box-footer"> Resolver tareas <i class="fa fa-arrow-circle-right"></i></a>
                        }
                    </div>
                </div>

                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-aqua">
                        <div class="inner">
                            <h3>@Model.TotalTareaPendiente</h3>

                            <p>En proceso</p>
                        </div>
                        <div class="icon">
                            <i class="ion-ios-folder"></i>
                        </div>
                        @if (Model.IdUsuario == Web.Helps.SessionTareas.IdUsuario)
                        {
                            <a href="@Url.Action("Buzon_entrada","Buzon" , new { Estado="A"})" class="small-box-footer"> Resolver tareas <i class="fa fa-arrow-circle-right"></i></a>
                        }
                        else
                        {
                            <a href="#" class="small-box-footer"> Resolver tareas <i class="fa fa-arrow-circle-right"></i></a>
                        }
                    </div>
                </div>
            </div>


